  # Include this snippet in your map file e.g. 
  # include 'vector_layers.map.include'
  LAYER
    NAME 'country'
    TYPE POLYGON
    CONNECTIONTYPE postgis
    CONNECTION "dbname='gis' host=elephant port=5432 user='xxxxxxx' password='xxxxxxx' sslmode=disable"
    # For gcgid connection pooling
    PROCESSING "CLOSE_CONNECTION=DEFER"
    DATA 'the_geom FROM country using unique gid using SRID=4326'
    METADATA
      'wms_title' 'country'
      'wms_srs'             'EPSG:4326'
      'wms_extent' "16 -35 33 -22"
    END
    STATUS DEFAULT
    TRANSPARENCY ALPHA
    PROJECTION
    'proj=longlat'
    'ellps=WGS84'
    'datum=WGS84'
    'no_defs'
    ''
    END
    CLASS
       NAME 'country' 
       STYLE
         ANTIALIAS TRUE
         SYMBOL 0 
         SIZE 2 
         OUTLINECOLOR 0 0 0
         COLOR -1 -1 -1
       END
    END
  END



