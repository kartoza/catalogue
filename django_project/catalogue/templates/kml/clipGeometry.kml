{% extends "kml/base.kml" %}
{% load url from future %} {# https://docs.djangoproject.com/en/1.4/releases/1.3/#changes-to-url-and-ssi #}
{% block records %}
<Placemark>
    <name>Clip geometry for order {{ order.id }} </name>
    <description>http://{{ external_site_url }}{% url 'viewOrder' order.id %}</description>
    
    {{ order.delivery_detail.geometry.kml|safe }}
    </Placemark>
{% endblock %}
